      </Label>
      <Label layoutX="201.0" layoutY="112.0" text="Username:">
         <font>
            <Font size="14.0" />
         </font>
      </Label>
      <Label layoutX="201.0" layoutY="144.0" text="Password:">
         <font>
            <Font size="14.0" />
         </font>
      </Label>
      <TextField fx:id="Username" layoutX="311.0" layoutY="110.0" />
      <PasswordField fx:id="Password" layoutX="311.0" layoutY="142.0" />
      <Button layoutX="274.0" layoutY="200.0" mnemonicParsing="false" onAction="#onLoginAction" text="Log In">
