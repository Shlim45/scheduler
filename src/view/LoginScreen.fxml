<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/11.0.2" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.LoginScreen">
   <children>
      <Label fx:id="Header" layoutX="215.0" layoutY="50.0" prefHeight="27.0" prefWidth="200.0" text="Enter user credentials">
         <font>
            <Font size="18.0" />
         </font>
      </Label>
      <Label fx:id="User" layoutX="187.0" layoutY="112.0" prefHeight="20.0" prefWidth="80.0" text="Username:">
         <font>
            <Font size="14.0" />
         </font>
      </Label>
      <Label fx:id="Pass" layoutX="189.0" layoutY="144.0" prefHeight="20.0" prefWidth="74.0" text="Password:">
         <font>
            <Font size="14.0" />
         </font>
      </Label>
      <TextField fx:id="Username" layoutX="311.0" layoutY="110.0" onAction="#onEnterAction" />
      <PasswordField fx:id="Password" layoutX="311.0" layoutY="142.0" onAction="#onEnterAction" />
      <Button fx:id="Login" layoutX="274.0" layoutY="200.0" mnemonicParsing="false" onAction="#onLoginAction" text="Log In">
         <font>
            <Font size="18.0" />
         </font>
      </Button>
      <Label fx:id="Location" layoutX="162.0" layoutY="296.0" prefHeight="20.0" prefWidth="325.0">
         <font>
            <Font size="14.0" />
         </font>
      </Label>
      <Label fx:id="Information" layoutX="157.0" layoutY="332.0" prefHeight="39.0" prefWidth="325.0">
         <font>
            <Font size="14.0" />
         </font>
      </Label>
   </children>
</AnchorPane>
